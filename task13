 public static void main(String args[]) {
 System.out.println("7. Вычислить значения функции на отрезке [а,b] c шагом h");
        int a = -3;
        int b = 9;
        int h = 2;
        int y;

        for (int i = a; i <= b; i += h) {
            if (i > 2) {
                y = i;
            } else {
                y = i * (-1);
            }
            System.out.println("При х = " + i + " у = " + y);
        }
        
