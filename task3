public class main {
public static void main(String[] args) {
        double x = 2;
        double y = 3.5;
        double val = ((Math.sin(x) + Math.cos(y)) / (Math.cos(x) - Math.sin(y)) * Math.tan(x * y);
         System.out.println("Числа x = " + x + ", y = " + y );
         System.out.println("Результат" + val);
        }
    }
